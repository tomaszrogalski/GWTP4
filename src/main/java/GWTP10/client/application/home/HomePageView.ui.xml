<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.html_panel {
		background-color: aqua;
		width: 100%;
		height: 500px;
		padding: 15px;
	}
	
	.horizontal_panel {
		padding: 25px;
	}
	
	.label {
		width: 150px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.html_panel}">
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="IMIE:" ui:field="labelImie" styleName="{style.label}"></g:Label>
			<g:TextBox text="IMIE" ui:field="textboxImie"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="NAZWISKO:" ui:field="labelNazwisko" styleName="{style.label}"></g:Label>
			<g:TextBox text="NAZWISKO" ui:field="textboxNazwisko"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="PESEL:" ui:field="labelPesel" styleName="{style.label}"></g:Label>
			<g:TextBox text="PESEL" ui:field="textboxPesel"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="ULICA:" ui:field="labelUlica" styleName="{style.label}"></g:Label>
			<g:TextBox text="ULICA" ui:field="textboxUlica"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="NR DOMU:" ui:field="labelNrDomu" styleName="{style.label}"></g:Label>
			<g:TextBox text="NR DOMU" ui:field="textboxNrDomu"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Label text="MIEJSCOWOSC:" ui:field="labelMiejscowosc"
				styleName="{style.label}"></g:Label>
			<g:TextBox text="MIEJSCOWOSC" ui:field="textboxMiejscowosc"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName="{style.horizontal_panel}">
			<g:Button text="WYCZYSC FORMULARZ" ui:field="buttonWyczysc"></g:Button>
			<g:Button text="WYSLIJ" ui:field="buttonWyslij"></g:Button>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder>

